{
  "name": "@fictjs/shadcn",
  "version": "0.1.0",
  "description": "Official shadcn-style code distribution system for Fict",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "fictcn": "./dist/bin/fictcn.js"
  },
  "files": [
    "dist",
    "schemas"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "clean": "rimraf dist coverage",
    "dev": "tsup --config tsup.config.ts --watch",
    "lint": "eslint .",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:smoke": "storybook dev --ci --smoke-test",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "diff": "^8.0.2",
    "jsonc-parser": "^3.3.1",
    "picocolors": "^1.1.1",
    "semver": "^7.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@storybook/addon-a11y": "^8.6.16",
    "@storybook/addon-essentials": "^8.6.16",
    "@storybook/html": "^8.6.16",
    "@storybook/html-vite": "^8.6.16",
    "@types/node": "^22.13.8",
    "@types/semver": "^7.5.8",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.21.0",
    "fict": "^0.10.0",
    "postcss": "^8.5.3",
    "rimraf": "^6.0.1",
    "storybook": "^8.6.16",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.25.0",
    "vite": "^5.4.14",
    "vitest": "^3.0.7"
  },
  "keywords": [
    "fict",
    "shadcn",
    "ui",
    "cli",
    "registry"
  ]
}
